{
  "config": {
    "handlePrefix": "my-asset",
    "host": "local.mysite",
    "site": "http://local.mysite:8888",
    "port": "8888",
    "protocol": "http",
    "theme": "my-theme"
  },
  "assets": {
    "enqueue_block_editor_assets": [
      {
        "handle": "editor-js",
        "path": "/js/editor",
        "ext": "js"
      }
    ],
    "wp_enqueue_scripts": [
      {
        "path": "/js/screen",
        "ext": "js",
        "condition": ["is_user_logged_in", null, false]
      }
    ]
  }
}
